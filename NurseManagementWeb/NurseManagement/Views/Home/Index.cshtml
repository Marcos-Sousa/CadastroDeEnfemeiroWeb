@{
    ViewData["Title"] = "Home Page";
}
<br /><br /><br />
<div class="container-fluid">
    <section class="content">
        <div class="col-lg-6 col-xs-6">
            <div class="small-box bg-danger">
                <div class="inner">
                    <br />
                    <h1 class="text-left"> @ViewBag.Nurses</h1>
                    <h3 class="text-center text-black-50">Quantidade de Enfermeiros Cadastrados</h3>
                </div>
                <div class="icon text-right">
                    <i class="fa fa-users fa-5x"></i><br />
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-xs-6">
            <div class="small-box bg-primary">
                <div class="inner">
                    <br />
                    <h1 class="text-left"> @ViewBag.Hospitais</h1>
                    <h3 class="text-center">Quantidade de Hospitais Cadastrados</h3>
                </div>
                <div class="icon text-right">
                    <i class="fa fa-hospital-o fa-5x"></i><br />
                </div>
            </div>
        </div>
    </section>
</div>
